{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "name": "blog"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "entry": {
      "type": "richtext",
      "required": true
    },
    "date": {
      "type": "date",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "slug": {
      "type": "string",
      "unique": true
    }
  }
}
